void
example(int n)
{
	/*
	 * XXX: The '} else' looks strange in this style since the 'else' is
	 * not at the left margin of the code.
	 */
	if (n > 99) { print("large"); }
	else if (n > 9) { print("double-digit"); }
	else if (n > 0) print("positive");
	else { print("negative"); }
}
